{
  "name": "@newhighsco/prettier-config",
  "description": "New High Score shareable config for prettier",
  "version": "2.0.5",
  "author": "New High Score <hello@newhighsco.re>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/newhighsco/prettier-config.git"
  },
  "homepage": "https://github.com/newhighsco/prettier-config#readme",
  "bugs": {
    "url": "https://github.com/newhighsco/prettier-config/issues"
  },
  "main": "prettier.config.js",
  "scripts": {
    "test": "npm run lint",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint --cache --ext .js,.json .",
    "format": "npm-run-all --parallel format:*",
    "format:js": "npm run lint:js -- --fix"
  },
  "devDependencies": {
    "@commitlint/cli": "9.1.2",
    "@newhighsco/commitlint-config": "1.0.6",
    "@newhighsco/editor-config": "1.1.1",
    "@newhighsco/eslint-config": "2.2.24",
    "@newhighsco/release-config": "1.0.28",
    "eslint": "7.8.1",
    "husky": "4.3.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.1.1",
    "semantic-release": "17.1.1"
  },
  "peerDependencies": {
    "prettier": "2.1.1"
  },
  "commitlint": {
    "extends": [
      "@newhighsco"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@newhighsco"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "prettier": "./prettier.config.js",
  "release": {
    "extends": "@newhighsco/release-config"
  },
  "renovate": {
    "extends": [
      "@newhighsco"
    ],
    "packageRules": [
      {
        "packageNames": [
          "prettier"
        ],
        "semanticCommitType": "fix"
      }
    ]
  }
}
